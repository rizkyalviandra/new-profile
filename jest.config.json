{
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "moduleNameMapper": {
    "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
  },
  "collectCoverageFrom": [
    "<rootDir>/src/**/**.ts",
    "<rootDir>/src/**/**.tsx",
    "!<rootDir>/src/pages/_app.tsx"
  ]
}
